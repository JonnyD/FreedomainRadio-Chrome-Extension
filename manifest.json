{
"update_url":"http://clients2.google.com/service/update2/crx",
    "name": "FreedomainRadio",
    "version": "1.0",
    "manifest_version": 2,
    "description": "FreedomainRadio - The Largest and Most Popular Philosophy Conversation in the World!",
    "icons": { "16": "img/icon16.png",
        "48": "img/icon48.png",
        "117": "img/icon117.png" },
