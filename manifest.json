    "permissions": [
        "storage",
        "notifications",
        "background",
        "http://fdrpodcast.com/",
        "http://*.freedomainradio.com/",
        "http://gdata.youtube.com/feeds/api/users/stefbot/uploads?v=2&alt=jsonc",
        "http://pipes.yahoo.com/pipes/pipe.run?_id=e5f849875fd5f61b23e5d7f79873d9c9&_render=json"
    ]
